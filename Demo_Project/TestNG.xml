<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Login Suite">

<test name="BothCorrect">
<parameter name="username" value="Admin"></parameter>
<parameter name="password" value="admin"></parameter>
<classes>
<class name="loginTestCases.BothCorrect"></class>
</classes>
</test>

<test name="Both_In_Correct">
<parameter name="username" value="Admin1"></parameter>
<parameter name="password" value="admin1"></parameter>
<classes>
<class name="loginTestCases.Both_In_Correct"></class>
</classes>
</test>

<test name="CorrectPassword">
<parameter name="username" value="Admin1"></parameter>
<parameter name="password" value="admin"></parameter>
<classes>
<class name="loginTestCases.CorrectPassword"></class>
</classes>
</test>

<test name="CorrectUserName">
<parameter name="username" value="Admin"></parameter>
<parameter name="password" value="dummy"></parameter>
<classes>
<class name="loginTestCases.CorrectUserName"></class>
</classes>
</test>
</suite>